<!DOCTYPE html>
<html>
<head>
<title>Home Page</title>
<link rel='stylesheet' href='/stylesheets/style.css' />
<script
	src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script
	src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
<script
	src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js"></script>
<link rel="stylesheet"
	href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
<script src="angularjs/ui-bootstrap-tpls-0.13.4.js"></script>
<script src="/angular/EditProfileController.js"></script>
</head>
<body ng-app="editProfile" ng-controller="editProfileCtrl">
	<div class="row">
		<div class="col-md-1"></div>
		<div class="col-md-1">
					<div class="navbar-header">
						<a class="navbar-brand" href="#"> <img length="30px" height="30px" alt="Brand" src="/Images/fbbrand.jpg">
						</a>
				</div>
		</div>
		<div class="col-md-3">
			<form id="f1" method="get" action="loadUserProfile">
				<div class="row">
					<br />
					<div class="col-lg-12">
						<div class="input-group">
							<input type="text" class="form-control"
								placeholder="Search Facebook"> <span
								class="input-group-btn">
								<button class="btn btn-default" type="button">
									<span class="glyphicon glyphicon-search"></span>
								</button>
							</span>
						</div>
					</div>
					<!-- /input-group -->
				</div><br/>
			</form>
		</div>
		<!-- /.row -->
		</div>
		<div class="row2">
<div class="col-md-3">
</div>
<div class="tabbable">
		<ul class="nav nav-pills nav-stacked col-md-2">
		<br/><br/><br/>
		  <li class="active"><a href="#pane1" data-toggle="tab">Personal</a></li>
		  <li><a href="#pane2" data-toggle="tab">Work & Education</a></li>
		  <li><a href="#pane3" data-toggle="tab">Interests</a></li>
		  <li><a href="#pane4" data-toggle="tab">Life Events</a></li>
		</ul>
		<div class="tabcontent col-md-4">
		<br/>
				<div id="pane1" class="tab-pane active">
					<br/>
					<label><span class="glyphicons glyphicons-iphone"></span></label>
					<input id="mobno" ng-model="ngmobno" placeholder="Enter Mobile Number" class="form-control"/><br/>
					<center><button type="button" class="btn btn-primary pull-right" ng-click="addmobno()">Update</button></center><br/><br/>
				</div>
				<div id="pane2" class="tab-pane">
					<input id="work" ng-model="ngjob" placeholder="Enter Work or Education" class="form-control"/><br/>
					
					<input id="loc" ng-model="nglocation" placeholder="Where" class="form-control"/><br/>
								<div class="col-md-3">
									<input id="from" ng-model="ngfrom" placeholder="From" class="form-control"/>
								</div>
								<div class="col-md-3">
									<input id="to" ng-model="ngto" placeholder="To" class="form-control"/>
								</div><br/>
					<center><button type="button" class="btn btn-primary pull-right" ng-click="addWork()">Update</button></center><br/><br/>
				</div>
				<div id="pane3" class="tab-pane">
				<input id="interests" ng-model="nginterest" placeholder="Enter your interests" class="form-control"/><br/>
				<center><button type="button" class="btn btn-primary pull-right" ng-click="addInterest()">Update</button></center><br/><br/>
				</div>
				<div id="pane4" class="tab-pane">
				<input id="lifeevent" ng-model="nglifeevent" placeholder="Add a Life Event" class="form-control"/><br/>
				<input id="to" ng-model="ngwhen" placeholder="When" class="form-control"/>
						<br/>
				<center><button type="button" class="btn btn-primary pull-right" ng-click="addEvent()">Update</button></center><br/><br/>	
				</div>
		</div>
</div>
<div class="col-md-3">
</div>
</div>

</body>
</html>